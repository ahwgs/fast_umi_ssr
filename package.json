{
  "name": "fast_umi_ssr",
  "version": "1.0.0",
  "description": "umi ssr app",
  "main": "index.js",
  "repository": "git@github.com:ahwgs/fast_umi_ssr.git",
  "author": "ahwgs <ah_wgs@126.com>",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "app/**/*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "osdoc-lint": "^1.0.6"
  },
  "dependencies": {
    "prettier": "^2.0.5"
  }
}
